{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/firebase.js",
      "dest": "/firebase.js"
    },
    {
      "src": "/user_config.js",
      "dest": "/user_config.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}